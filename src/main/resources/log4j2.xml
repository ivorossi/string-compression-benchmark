<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
	<Appenders>
		<File name="File" fileName="work/logs/string-compression-benchmark-ivo.log">
			<PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss} %-5p [%c] %replace{%msg}{[\r\n]+}{}%n" />
		</File>
	</Appenders>
	<Loggers>
		<Root level="debug">
			<AppenderRef ref="File" />
		</Root>
	</Loggers>
</Configuration>